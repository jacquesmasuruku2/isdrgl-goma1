{
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "env": ["REACT_APP_SUPABASE_URL", "REACT_APP_SUPABASE_ANON_KEY", "REACT_APP_API_URL"],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
